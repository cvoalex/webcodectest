# OpenAI API Key - Get from https://platform.openai.com/settings/api-keys
OPENAI_API_KEY="your-api-key-here"

# Server Configuration
PORT=3000

# Audio Processing Configuration
AUDIO_SAMPLE_RATE=24000
AUDIO_CHUNK_DURATION_MS=40
AUDIO_BUFFER_SIZE=3000

# Frame Generation Configuration
FRAMES_FOR_INFERENCE=16
INFERENCE_BATCH_SIZE=5
LOOKAHEAD_FRAMES=7

# Service Endpoints
FRAME_GENERATOR_WS_PORT=8080
GRPC_SERVICE_HOST=localhost
GRPC_SERVICE_PORT=50051

# Performance Settings
TARGET_FPS=30
MAX_BUFFER_LATENCY_MS=500
FRAME_GENERATION_TIMEOUT_MS=5000

# Debug Settings
DEBUG_AUDIO_PROCESSING=false
DEBUG_FRAME_GENERATION=false
DEBUG_WEBSOCKET_MESSAGES=false
